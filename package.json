{
    "name": "@hckrnews/objects",
    "description": "Create valid JavaScript objects",
    "version": "6.0.0",
    "author": {
        "name": "Pieter Wigboldus",
        "url": "https://hckr.news/"
    },
    "license": "LGPL-3.0",
    "scripts": {
        "lint": "eslint src/*.js --config .eslintrc",
        "lint:report": "eslint src/*.js --config .eslintrc -f json -o report.json",
        "lint:fix": "eslint src/*.js --config .eslintrc --fix",
        "test": "c8 node --test src/",
        "cpd": "node_modules/jscpd/bin/jscpd src",
        "vulnerabilities": "npm audit --production",
        "build": "microbundle --target node src/objects.js"
    },
    "type": "module",
    "files": [
        "src/schemas/parse-options.js",
        "src/objects.js",
        "src/parser.js",
        "src/int.js",
        "dist/objects.cjs",
        "dist/objects.cjs.map",
        "dist/objects.js",
        "dist/objects.js.map",
        "dist/objects.module.mjs",
        "dist/objects.module.mjs.map",
        "dist/objects.umd.cjs",
        "dist/objects.umd.cjs.map"
    ],
    "main": "dist/objects.js",
    "source": "src/objects.js",
    "module": "dist/objects.module.mjs",
    "unpkg": "dist/objects.umd.cjs",
    "umd:main": "dist/objects.umd.cjs",
    "exports": {
        "node": {
            "module": "./src/objects.js",
            "require": "./dist/objects.umd.cjs"
        },
        "require": "./dist/objects.umd.cjs",
        "default": "./src/objects.js"
    },
    "devDependencies": {
        "@hckrnews/eslint-config": "^2.1.2",
        "c8": "^7.12.0",
        "eslint": "^8.18.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-html": "^6.1.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.0",
        "eslint-plugin-prettier": "^4.1.0",
        "jscpd": "^3.2.1",
        "microbundle": "^0.15.1",
        "prettier": "^2.2.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hckrnews/objects"
    },
    "engines": {
        "node": ">= 18"
    },
    "keywords": [
        "objects",
        "flat"
    ],
    "dependencies": {
        "@hckrnews/error": "^1.0.0",
        "@hckrnews/validator": "^8.0.1"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/w3nl"
    }
}
