{
    "name": "@hckrnews/objects",
    "description": "Create valid JavaScript objects",
    "version": "5.0.1",
    "author": {
        "name": "Pieter Wigboldus",
        "url": "https://hckr.news/"
    },
    "license": "LGPL-3.0",
    "scripts": {
        "lint": "eslint src/*.js --config .eslintrc",
        "lint:report": "eslint src/*.js --config .eslintrc -f json -o report.json",
        "lint:fix": "eslint src/*.js --config .eslintrc --fix",
        "test": "jest",
        "test:watch": "jest src --watch",
        "coveralls": "jest && codecov && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "cpd": "node_modules/jscpd/bin/jscpd src",
        "vulnerabilities": "npm audit --production"
    },
    "type": "module",
    "files": [
        "src/schemas/parse-options.js",
        "src/objects.js",
        "src/parser.js",
        "src/int.js"
    ],
    "main": "src/objects.js",
    "devDependencies": {
        "@babel/core": "^7.18.6",
        "@babel/eslint-parser": "^7.18.2",
        "@babel/plugin-transform-modules-commonjs": "^7.18.6",
        "@babel/plugin-transform-runtime": "^7.18.6",
        "@babel/preset-env": "^7.18.6",
        "@hckrnews/eslint-config": "^3.0.0",
        "@jest/globals": "^27.2.4",
        "babel-jest": "^27.2.4",
        "codecov": "^3.6.5",
        "coveralls": "^3.0.13",
        "eslint": "^8.18.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-html": "^6.1.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.0",
        "eslint-plugin-prettier": "^4.1.0",
        "jest": "^27.2.4",
        "jest-sonar-reporter": "^2.0.0",
        "jscpd": "^3.2.1",
        "prettier": "^2.2.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hckrnews/objects"
    },
    "engines": {
        "node": ">= 14"
    },
    "keywords": [
        "objects",
        "flat"
    ],
    "dependencies": {
        "@hckrnews/error": "^0.3.8",
        "@hckrnews/validator": "^7.0.0"
    }
}
