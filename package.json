{
    "name": "@hckrnews/objects",
    "description": "Create valid JavaScript objects",
    "version": "6.0.11",
    "author": {
        "name": "Pieter Wigboldus",
        "url": "https://hckr.news/"
    },
    "license": "MIT",
    "scripts": {
        "lint": "eslint src/*.js --config .eslintrc",
        "lint:report": "eslint src/*.js --config .eslintrc -f json -o report.json",
        "lint:fix": "eslint src/*.js --config .eslintrc --fix",
        "test": "c8 node --test src/__tests__/*.js",
        "cpd": "node_modules/jscpd/bin/jscpd src",
        "vulnerabilities": "npm audit --production",
        "build": "microbundle --target node src/objects.js",
        "postinstall": "node postinstall.js"
    },
    "type": "module",
    "files": [
        "src/schemas/parse-options.js",
        "src/objects.js",
        "src/parser.js",
        "src/int.js",
        "dist/objects.cjs",
        "dist/objects.cjs.map",
        "dist/objects.js",
        "dist/objects.js.map",
        "dist/objects.module.mjs",
        "dist/objects.module.mjs.map",
        "dist/objects.umd.cjs",
        "dist/objects.umd.cjs.map",
        "postinstall.js"
    ],
    "main": "dist/objects.umd.cjs",
    "source": "src/objects.js",
    "module": "dist/objects.module.mjs",
    "unpkg": "dist/objects.umd.cjs",
    "umd:main": "dist/objects.umd.cjs",
    "exports": {
        "node": {
            "default": "./dist/objects.umd.cjs",
            "module": "./src/objects.js",
            "require": "./dist/objects.umd.cjs"
        },
        "require": "./dist/objects.umd.cjs",
        "default": "./dist/objects.umd.cjs"
    },
    "devDependencies": {
        "@hckrnews/eslint-code-quality": "^0.1.5",
        "@hckrnews/eslint-config": "^3.0.0",
        "c8": "^8.0.0",
        "eslint": "^8.18.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-html": "^7.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jscpd": "^3.2.1",
        "microbundle": "^0.15.1",
        "prettier": "^3.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hckrnews/objects"
    },
    "engines": {
        "node": ">= 18.13"
    },
    "keywords": [
        "objects",
        "flat"
    ],
    "dependencies": {
        "@hckrnews/error": "^1.0.1",
        "@hckrnews/validator": "^8.0.5"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/w3nl"
    },
    "overrides": {
        "xml2js": "^0.5.0"
    }
}
