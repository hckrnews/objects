{
    "name": "@hckrnews/objects",
    "description": "Create valid JavaScript objects",
    "version": "3.3.9",
    "author": {
        "name": "Pieter Wigboldus",
        "url": "https://hckr.news/"
    },
    "license": "LGPL-3.0",
    "scripts": {
        "lint": "eslint src/*.js --config .eslintrc -f json -o report.json",
        "lint:fix": "eslint src/*.js --config .eslintrc --fix",
        "test": "jest",
        "test:watch": "jest src --watch",
        "coveralls": "jest && codecov && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "cpd": "node_modules/jscpd/bin/jscpd src",
        "vulnerabilities": "npm audit --only=prod"
    },
    "type": "module",
    "files": [
        "src/schemas/parse-options.js",
        "src/objects.js",
        "src/parser.js",
        "src/int.js"
    ],
    "main": "src/objects.js",
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/eslint-parser": "^7.16.3",
        "@babel/plugin-transform-modules-commonjs": "^7.14.0",
        "@babel/preset-env": "^7.16.4",
        "@hckrnews/eslint-config": "^2.0.0",
        "@jest/globals": "^27.4.2",
        "babel-jest": "^27.4.2",
        "codecov": "^3.6.5",
        "coveralls": "^3.0.13",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^19.0.1",
        "eslint-config-prettier": "^8.2.0",
        "eslint-plugin-html": "^6.1.2",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^3.4.1",
        "jest": "^27.4.2",
        "jest-sonar-reporter": "^2.0.0",
        "jscpd": "^3.4.2",
        "prettier": "^2.5.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hckrnews/objects"
    },
    "engines": {
        "node": ">= 14"
    },
    "keywords": [
        "objects",
        "flat"
    ],
    "dependencies": {
        "@hckrnews/validator": "^5.0.4"
    }
}
