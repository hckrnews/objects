    "type": "module",
    "files": [
        "src/schemas/parse-options.js",
        "src/objects.js",
        "src/parser.js",
        "src/int.js"
    ],
    "main": "src/objects.js",
    "devDependencies": {
        "@hckrnews/eslint-config": "^2.1.2",
        "c8": "^7.12.0",
        "eslint": "^8.18.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-html": "^6.1.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.0",
        "eslint-plugin-prettier": "^4.1.0",
        "jscpd": "^3.2.1",
        "prettier": "^2.2.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hckrnews/objects"
    },
    "engines": {
        "node": ">= 18"
    },
    "keywords": [
        "objects",
        "flat"
    ],
    "dependencies": {
        "@hckrnews/error": "^0.3.8",
        "@hckrnews/validator": "^7.0.0"
    }
