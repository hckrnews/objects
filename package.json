{
    "name": "@hckrnews/objects",
    "description": "Hack JavaScript objects",
    "version": "2.1.3",
    "author": {
        "name": "Pieter Wigboldus",
        "url": "https://hckr.news/"
    },
    "license": "LGPL-3.0",
    "scripts": {
        "build": "microbundle --target node",
        "dev": "microbundle --target node watch",
        "lint": "eslint src/*.js --config .eslintrc",
        "lint:fix": "eslint src/*.js --config .eslintrc --fix",
        "test": "jest",
        "test:watch": "jest src --watch",
        "coveralls": "jest && codecov && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "cpd": "node_modules/jscpd/bin/jscpd src"
    },
    "files": [
        "src/objects.js",
        "dist/objects.js",
        "dist/objects.js.map",
        "dist/objects.module.mjs",
        "dist/objects.module.mjs.map",
        "dist/objects.modern.mjs",
        "dist/objects.modern.mjs.map",
        "dist/objects.umd.cjs",
        "dist/objects.umd.cjs.map"
    ],
    "source": "src/objects.js",
    "main": "dist/objects.js",
    "module": "dist/objects.module.mjs",
    "esmodule": "dist/objects.modern.mjs",
    "unpkg": "dist/objects.umd.cjs",
    "umd:main": "dist/objects.umd.cjs",
    "exports": {
        ".": {
            "require": "./dist/objects.umd.cjs",
            "import": "./dist/objects.module.mjs",
            "default": "./dist/objects.js"
        },
        "./source": "./src/objects.js",
        "./main": "./dist/objects.js",
        "./module": "./dist/objects.module.mjs",
        "./modern": "./dist/objects.modern.mjs",
        "./umd": "./dist/objects.umd.cjs"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.5",
        "@hckrnews/eslint-config": "^0.2.0",
        "babel-eslint": "^10.1.0",
        "codecov": "^3.6.5",
        "coveralls": "^3.0.13",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-html": "^6.0.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-vue": "^6.2.2",
        "jest": "^26.6.3",
        "jscpd": "^3.2.1",
        "microbundle": "^0.13.0",
        "prettier": "^1.19.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hckrnews/objects"
    },
    "engines": {
        "node": ">= 14"
    },
    "keywords": [
        "objects",
        "flat"
    ],
    "dependencies": {
        "@hckrnews/validator": "^2.1.7"
    }
}
